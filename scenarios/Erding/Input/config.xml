    <module name="transitRouter">
        <param name="searchRadius" value="1000"/>
        <param name="extensionRadius" value="200"/>
        <param name="maxBeelineWalkConnectionDistance" value="200"/>
        <param name="additionalTransferTime" value="120"/>
    </module>
    <module name="controler" >
        <param name="lastIteration" value="5" />
        <param name="runId" value="Erding-Matsim" />
        <param name="outputDirectory" value="./scenarios/Erding/output/"/>
        <param name="writeEventsInterval" value="50" />
        <param name="writePlansInterval" value="50" />
        <param name="routingAlgorithmType" value= "FastAStarLandmarks"/>
    </module>
    <module name="global" >
